     <rule class="net.sourceforge.pmd.rules.XPathRule" dfa="false" externalInfoUrl="" message="Position literals first in String comparisons" name="PositionLiteralsFirstInComparisons" typeResolution="true">
      <description>that way if the String is null you won't get a NullPointerException, it'll just return false.</description>
      <priority>1</priority>
      <properties>
         <property name="xpath">
            <value><![CDATA[//PrimaryExpression[

        PrimaryPrefix[Name

                [

	(ends-with(@Image, '.equals'))

                ]

        ]

        [

                   (../PrimarySuffix/Arguments/ArgumentList/Expression/PrimaryExpression/PrimaryPrefix/Literal)

	and

	( count(../PrimarySuffix/Arguments/ArgumentList/Expression) = 1 )

        ]

]

[not(ancestor::Expression/ConditionalAndExpression//EqualityExpression[@Image='!=']//NullLiteral)]

[not(ancestor::Expression/ConditionalOrExpression//EqualityExpression[@Image='==']//NullLiteral)]]]></value>
         </property>
      </properties>
      <example><![CDATA[class Foo {

 boolean bar(String x) {

  return x.equals("2"); // should be "2".equals(x)

 }

}]]></example>
      <example><![CDATA[class Foo {

 boolean bar(String x) {

  return x.equals("2"); // should be "2".equals(x)

 }

}]]></example>
